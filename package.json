{
    "name": "node-unifiapi",
    "version": "0.0.54",
    "description": "Provides API to manage Ubiquiti Unifi Controller, ver 4 and 5",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
        "lint": "tslint -p tsconfig.json",
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "tsc",
        "prepare": "npm run build",
        "readme": "jsdoc2md --template README.hbs --files index.js cloudapi.js > README.md"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/delian/node-unifiapi.git"
    },
    "keywords": [
        "unifi",
        "ubiquiti"
    ],
    "author": "Delian Delchev",
    "license": "GPL-2.0",
    "bugs": {
        "url": "https://github.com/delian/node-unifiapi/issues"
    },
    "homepage": "https://github.com/delian/node-unifiapi#readme",
    "devDependencies": {
        "@types/debug": "^4.1.5",
        "@types/pako": "^1.0.1",
        "@types/request": "^2.48.5",
        "@types/request-debug": "^0.2.0",
        "@types/uuid": "^8.0.1",
        "jsdoc-to-markdown": "^3.0.4",
        "prettier": "^2.0.5",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.9.7"
    },
    "dependencies": {
        "debug": "^2.6.9",
        "merge": "^1.2.1",
        "pako": "^1.0.11",
        "request": "^2.88.2",
        "request-debug": "^0.2.0",
        "uuid": "^8.3.0",
        "ws": "^7.3.1"
    },
    "jshintConfig": {
        "esversion": 6
    },
    "engines": {
        "node": ">=4.1.0"
    }
}
